<h1>Welcome</h1>
<h1>Listing Student Details</h1>
 <%= link_to 'New student', new_welcome_path %>
<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Phoneno</th>
    <th colspan="3"></th>
  </tr>
 
<% @welcome.each do |welcome| %>
  <tr>
    <td><%= welcome.name %></td>
    <td><%= welcome.address %></td>
    <td><%= welcome.phoneno %></td>
    <td><%= link_to 'Show', welcome_path(welcome) %></td>
    <td><%= link_to 'Delete', welcome_path(welcome), method: :delete, data: {confirm: 'Are u sure'}%></td>
    
  </tr>
<% end %>
</table>
